!
! Chromaticity sextupoles in the arcs
!
LSEX= 0.1524
sexend: DRIFT, L = 0.028575
sexslot: DRIFT, L = lsex + 2*sexend[l]
! GEneric sextupoles , generally not used anywhere (except teapot???)
SEXH: SEXTUPOLE, TYPE = XCHR, L = LSEX , K2 = k2f 
SEXV: SEXTUPOLE, TYPE = YCHR, L = LSEX , K2 = k2d 
