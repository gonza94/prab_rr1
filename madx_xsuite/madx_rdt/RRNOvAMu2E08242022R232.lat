!LRKD25_UEND := 0.0433838 !1.712 inches
!LRKD25_DEND := 0.0530352 !2.088 inches
!RKD25_UEND: DRIFT, L = LRKD25_case + LRKD25_UEND !
!RKD25_DEND: DRIFT, L = LRKD25_case + LRKD25_DEND !

LRKD25_C2UF := 0.577343  !22.73"
LRKD25_C2DF := 0.588011  !23.15"
RKD25_UEND: DRIFT, L = LRKD25_C2UF - LRKD25_MAG/2 !
RKD25_DEND: DRIFT, L = LRKD25_C2DF - LRKD25_MAG/2 !

LRKD25_SLOT = LRKD25_MAG + RKD25_UEND[L] + RKD25_DEND[L] !1.165759m, 45.9"
!
